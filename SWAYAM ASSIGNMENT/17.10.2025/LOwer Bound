class Solution {
    int lowerBound(int[] arr, int target) {
        // code here
        int beg = 0,end = arr.length-1;
        while(beg<=end){
            int mid = beg+(end-beg)/2;
            if(arr[mid]>=target){
                end = mid-1;
            }else{
                beg = mid+1;
            }
        }return beg;
    }
}
