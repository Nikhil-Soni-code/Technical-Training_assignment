class myQueue {

    // Constructor
    int[] a;
    int front,rear,count;
    public myQueue(int n) {
        // Define Data Structures
        a = new int[n];
        front = 0;
        rear = 0;
        count=0;
    }

    public boolean isEmpty() {
        // Check if queue is empty
        if(count==0)return true;
        return false;
        
    }

    public boolean isFull() {
        // Check if queue is full
        return count == a.length;
    }

    public void enqueue(int x) {
        // Enqueue
        if(!isFull()){
            count++;
            a[rear] = x;
            rear = (rear+1)%a.length;
        }
    }

    public void dequeue() {
        // Dequeue
        if(!isEmpty()){
            count--;
            front = (front+1)%a.length;
        }
    }

    public int getFront() {
        // Get front element
        if(!isEmpty()){
            return a[front];
        }return -1;
    }

    public int getRear() {
        // Get last element
        if(!isEmpty()){
            if(rear==0){
                return a[a.length-1];
            }else{
                return a[rear-1];
            }
        }return -1;
    }
}
