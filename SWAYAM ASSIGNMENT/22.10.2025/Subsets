class Solution {
    private void generate(List<List<Integer>> ans,List<Integer> subList,int[]nums,int i){
        if(i==nums.length){
            ans.add(new ArrayList(subList));
            return;
        }
        subList.add(nums[i]);
        generate(ans,subList,nums,i+1);
        subList.remove(subList.size()-1);
        generate(ans,subList,nums,i+1);

    }
    public List<List<Integer>> subsets(int[] nums) {
        List<List<Integer>> ans = new ArrayList();
        generate(ans,new ArrayList(),nums,0);
        return ans;
    }
}
