class Solution {
    public static int countSetBits(int n) {
        // code here
        int[] ans = new int[n+1];
        ans[0] = 0;
        ans[1] = 1;
        int sum = 1;
        for(int i=2;i<=n;i++){
            if(i%2==0)ans[i] = ans[i/2];
            else ans[i] = ans[i-1]+1;
            sum += ans[i];
        }
        return sum;

    }
    
}
